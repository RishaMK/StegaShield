{
  "name": "stegashield",
  "displayName": "StegaShield",
  "description": "A VSCode extension that uses steganography to secure your data",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extractApiKey",
        "title": "Edit .env"
      },
      {
        "command": "extension.hideApiKey",
        "title": "Hide .env"
      },
      {
        "command": "stegashield.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0"
  },
  "dependencies": {
    "pngjs": "^7.0.0"
  }
}
